./mk_clean
echo "building mujs"
cd mujs-1.1.3/
gcc -m32 -Og -g one.c main.c -lm -ldl -o ../artifacts/mujs
cd ..
pushd .
echo "running mujs"
cd ../../tcc_bootstrap_alt/m2min/
./mk_clean
cat M2_simple_asm.c simple_support_m2.c > ./artifacts/M2_simple_asm_m2.c
../../tcc_simple/experiments/artifacts/mujs ../../tcc_simple/experiments/m2_mujs_min.js &> ../../tcc_simple/experiments/artifacts/M2_simple_asm_orig.M1
popd
sha256sum -c sums_mujs
