rm -r artifacts/builds/pnut_js

set -xe

BLD_DIR=artifacts/builds/pnut_js
SRC_DIR=artifacts/deps

mkdir $BLD_DIR

./mk_full_deps

./mk_full_cc_x86_min

source ./mk_check

pushd .
cd ..
./mk_clean
popd

cp ./artifacts/builds/pnut_js/pnut_js.exe ../artifacts/pnut-exe
cd ..
./mk_tcc-boot-mes

echo "DONE ALL"
