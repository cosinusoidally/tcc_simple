./mk_clean
set -xe
cat pnut_refactor/simple_support_js_m2.js pnut_refactor/pnut.js > artifacts/out.js
js_to_c_cc_x86.exe artifacts/out.js artifacts/out.c
tcc -E artifacts/out.c > artifacts/out2.c
cat pnut_refactor/simple_support_js_m2_prims.c pnut_refactor/support_common.c artifacts/out2.c > artifacts/out3.c
time M2-Planet --bootstrap-mode --architecture x86 --max-string 65535 -f artifacts/out3.c -o artifacts/pnut_m2.M1
time cc_x86 artifacts/out3.c artifacts/pnut_cc_x86.M1

# diff -u -s artifacts/pnut_m2.M1 artifacts/pnut_cc_x86.M1

cd artifacts

cat ../utils/x86_defs.M1 ../utils/libc-core.M1 pnut_m2.M1 > pnut-0.M1

M0 pnut-0.M1 pnut-0.hex2

cat ../utils/ELF-i386.hex2 pnut-0.hex2 > pnut-0-0.hex2

hex2-0 pnut-0-0.hex2 pnut_js.exe

chmod +x pnut_js.exe

wc *M1
wc *hex2
